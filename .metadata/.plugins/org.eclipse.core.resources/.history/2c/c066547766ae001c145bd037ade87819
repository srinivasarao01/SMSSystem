package com.test.testappl;

import org.springframework.beans.factory.annotation.Value;

public class TestA {

	@Value(${"name"})
	private String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "TestA [name=" + name + "]";
	}

}
